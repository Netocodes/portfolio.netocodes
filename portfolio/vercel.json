{
  "redirects": [
    {
      "source": "https://netocodes.pro/:path*",
      "destination": "https://www.netocodes.pro/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "netocodes.pro"
        }
      ],
      "destination": "https://www.netocodes.pro/:path*",
      "permanent": true
    }
  ]
}
